  "main": "src/index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "test": "jest --config=jest.config.cjs",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest tests/integration --config=jest.integration.config.js",
    "test:load": "node tests/load/load-runner.js",
    "test:security": "node tests/security/security-suite.js",
    "test:quantum": "jest tests/quantum --config=jest.quantum.config.js",
    "test:all": "jest --coverage --verbose",
    "docs": "node src/docs/cli/generate-docs.js",
    "docs:watch": "nodemon --watch src --ext js,md --exec npm run docs",
    "execute-protocol": "node src/cli/execute-hdr-protocol.js",
    "hdr:protocol": "node src/cli/execute-hdr-protocol.js",
    "hdr:full": "node src/cli/execute-hdr-protocol.js",
    "docker:build": "docker build -t hdr-empire:latest .",
    "docker:run": "docker run -d -p 3000:3000 --name hdr-empire hdr-empire:latest",
    "docker:stop": "docker stop hdr-empire && docker rm hdr-empire",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:prod": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "k8s:deploy": "bash deployment/scripts/deploy.sh",
    "k8s:rollback": "bash deployment/scripts/rollback.sh",
    "k8s:health": "bash deployment/scripts/health-check.sh"
  },
