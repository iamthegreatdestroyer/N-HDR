{
  "dashboard": {
    "id": null,
    "uid": "hdr-empire-main",
    "title": "HDR Empire System Dashboard",
    "tags": ["hdr-empire", "production", "consciousness", "quantum"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
    },
    "panels": [
      {
        "id": 1,
        "title": "HDR System Status",
        "type": "gauge",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "hdr_system_status{system=\"neural-hdr\"}",
            "legendFormat": "Neural-HDR",
            "refId": "A"
          },
          {
            "expr": "hdr_system_status{system=\"nano-swarm\"}",
            "legendFormat": "Nano-Swarm",
            "refId": "B"
          },
          {
            "expr": "hdr_system_status{system=\"omniscient-hdr\"}",
            "legendFormat": "Omniscient-HDR",
            "refId": "C"
          },
          {
            "expr": "hdr_system_status{system=\"void-blade\"}",
            "legendFormat": "Void-Blade",
            "refId": "D"
          },
          {
            "expr": "hdr_system_status{system=\"reality-hdr\"}",
            "legendFormat": "Reality-HDR",
            "refId": "E"
          },
          {
            "expr": "hdr_system_status{system=\"quantum-hdr\"}",
            "legendFormat": "Quantum-HDR",
            "refId": "F"
          },
          {
            "expr": "hdr_system_status{system=\"dream-hdr\"}",
            "legendFormat": "Dream-HDR",
            "refId": "G"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "green", "value": 1 }
              ]
            },
            "min": 0,
            "max": 1
          }
        }
      },
      {
        "id": 2,
        "title": "Operation Latency (Avg 5min)",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
        "targets": [
          {
            "expr": "rate(hdr_system_latency_seconds_sum[5m]) / rate(hdr_system_latency_seconds_count[5m])",
            "legendFormat": "{{system}} - {{operation}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency",
            "logBase": 1,
            "show": true
          },
          {
            "format": "short",
            "logBase": 1,
            "show": true
          }
        ]
      },
      {
        "id": 3,
        "title": "System Operations Rate",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
        "targets": [
          {
            "expr": "rate(hdr_system_operations_total[5m])",
            "legendFormat": "{{system}} - {{operation}} ({{status}})",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Operations/sec",
            "logBase": 1,
            "show": true
          },
          {
            "format": "short",
            "logBase": 1,
            "show": true
          }
        ]
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
        "targets": [
          {
            "expr": "rate(hdr_system_errors_total[5m])",
            "legendFormat": "{{system}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "errors/sec",
            "label": "Errors/sec",
            "logBase": 1,
            "show": true
          },
          {
            "format": "short",
            "logBase": 1,
            "show": true
          }
        ],
        "alert": {
          "name": "High Error Rate",
          "conditions": [
            {
              "evaluator": {
                "params": [1],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "frequency": "1m",
          "handler": 1,
          "message": "HDR System error rate is above threshold",
          "noDataState": "no_data",
          "executionErrorState": "alerting"
        }
      },
      {
        "id": 5,
        "title": "Consciousness Layers",
        "type": "stat",
        "gridPos": { "h": 4, "w": 6, "x": 0, "y": 16 },
        "targets": [
          {
            "expr": "hdr_consciousness_layers",
            "legendFormat": "{{system}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 4 },
                { "color": "red", "value": 8 }
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Quantum Coherence",
        "type": "gauge",
        "gridPos": { "h": 4, "w": 6, "x": 6, "y": 16 },
        "targets": [
          {
            "expr": "hdr_quantum_state",
            "legendFormat": "{{system}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "yellow", "value": 0.5 },
                { "color": "green", "value": 0.8 }
              ]
            },
            "min": 0,
            "max": 1
          }
        }
      },
      {
        "id": 7,
        "title": "Storage Efficiency",
        "type": "gauge",
        "gridPos": { "h": 4, "w": 6, "x": 12, "y": 16 },
        "targets": [
          {
            "expr": "hdr_storage_efficiency",
            "legendFormat": "{{system}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "yellow", "value": 0.7 },
                { "color": "green", "value": 0.9 }
              ]
            },
            "min": 0,
            "max": 1
          }
        }
      },
      {
        "id": 8,
        "title": "Security Level",
        "type": "stat",
        "gridPos": { "h": 4, "w": 6, "x": 18, "y": 16 },
        "targets": [
          {
            "expr": "hdr_security_level",
            "legendFormat": "{{system}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "yellow", "value": 3 },
                { "color": "green", "value": 7 }
              ]
            },
            "min": 0,
            "max": 9
          }
        }
      },
      {
        "id": 9,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
        "targets": [
          {
            "expr": "process_resident_memory_bytes",
            "legendFormat": "Resident Memory",
            "refId": "A"
          },
          {
            "expr": "process_heap_bytes",
            "legendFormat": "Heap",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "bytes",
            "label": "Memory",
            "logBase": 1,
            "show": true
          }
        ]
      },
      {
        "id": 10,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total[5m])",
            "legendFormat": "CPU Usage",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percentunit",
            "label": "CPU %",
            "logBase": 1,
            "show": true
          }
        ]
      }
    ]
  }
}
